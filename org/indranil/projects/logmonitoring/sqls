SQL Queries
 use [Hibernate-Tutorial]
 create table LOG_MONITORING_PROPERTIES (id int primary key, LOG_PATH varchar(1000), LOG_NAME varchar(100),SEARCH_PATTERN varchar(200))
 drop table LOG_MONITORING_PROPERTIES
 delete from LOG_MONITORING_PROPERTIES
 insert into LOG_MONITORING_PROPERTIES values(1,'C:\Users\user\Desktop\','IntelGFX.log','for delete on uninstall') 
 create table LOG_MONITORING_RESULT (id int, RESULT varchar(1000))
 select * from LOG_MONITORING_RESULT
 delete from LOG_MONITORING_RESULT
 create table COLLINE_LOG_MONITORING_PROPERTIES (PROCESS varchar(100),SLA varchar(20),ACTUAL_TIMESTAMP_STRATEGY varchar(100), ACTUAL_TIMESTAMP_PATTERN varchar(100)
 create table COLLINE_LOG_MONITORING_RESULT (PROCESS varchar(100),SLA varchar(20),ACTUAL_TIMESTAMP varchar(20), SLA_MET char)
 drop table COLLINE_LOG_MONITORING_PROPERTIES
 insert into COLLINE_LOG_MONITORING_PROPERTIES values('APAC LOG Check','3:00 AM','1','dd/MM/yyyy HH:mm a')
 delete from COLLINE_LOG_MONITORING_PROPERTIES
 select * from COLLINE_LOG_MONITORING_PROPERTIES
 select * from COLLINE_LOG_MONITORING_RESULT
 delete from COLLINE_LOG_MONITORING_RESULT